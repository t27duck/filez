<div id="<%= dom_id user %>">
  <p>
    <strong>Username:</strong>
    <%= user.username %>
  </p>

  <p>
    <strong>Password digest:</strong>
    <%= user.password_digest %>
  </p>

  <p>
    <strong>Token:</strong>
    <%= user.token %>
  </p>

  <p>
    <strong>Upload key:</strong>
    <%= user.upload_key %>
  </p>

  <p>
    <strong>Admin:</strong>
    <%= user.admin %>
  </p>

  <p>
    <strong>Default private:</strong>
    <%= user.default_private %>
  </p>

  <p>
    <strong>Default expiration duration:</strong>
    <%= user.default_expiration_duration %>
  </p>

  <p>
    <strong>Default download limit:</strong>
    <%= user.default_download_limit %>
  </p>

</div>
